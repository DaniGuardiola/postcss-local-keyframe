{
  "name": "postcss-prefix-keyframe",
  "version": "0.0.1",
  "description": "PostCSS plugin for prefixing keyframes",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "prefix-keyframe",
    "prefix-animation"
  ],
  "scripts": {
    "test": "jest --coverage && eslint ."
  },
  "author": "Vitaliy Ribachenko <vit@ribachenko.com>",
  "license": "MIT",
  "repository": "VitaliyR/postcss-prefix-keyframe",
  "engines": {
    "node": ">=10.0.0"
  },
  "peerDependencies": {
    "postcss": "^8.2.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "clean-publish": "^5.0.0",
    "eslint": "^9.3.0",
    "eslint-plugin-jest": "^28.5.0",
    "globals": "^15.3.0",
    "lint-staged": "^15.2.5",
    "postcss": "^8.4.38",
    "simple-git-hooks": "^2.11.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:jest/recommended"
    ],
    "rules": {
      "jest/expect-expect": "off"
    }
  }
}
